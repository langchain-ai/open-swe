{
  "name": "@open-swe/shared",
  "homepage": "https://github.com/langchain-ai/agent-mojo/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/langchain-ai/agent-mojo.git"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./dist .turbo tsconfig.tsbuildinfo || true",
    "build": "yarn clean && tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "NODE_OPTIONS=--experimental-vm-modules yarn run jest --config jest.config.js --testPathIgnorePatterns=int.test.ts",
    "test:int": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.js --testPathPattern=int.test.ts",
    "test:single": "NODE_OPTIONS=--experimental-vm-modules yarn run jest --config jest.config.js --testTimeout 100000"
  },
  "dependencies": {
    "@langchain/core": "^0.3.65",
    "@langchain/langgraph": "^0.3.8",
    "@langchain/langgraph-sdk": "^0.0.95",
    "@octokit/rest": "^22.0.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.19.0",
    "@jest/globals": "^29.7.0",
    "@octokit/types": "^12.0.0",
    "@tsconfig/recommended": "^1.0.8",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.13.5",
    "dotenv": "^16.4.7",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.1.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0"
  },
  "packageManager": "yarn@3.5.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./constants": {
      "import": "./dist/constants.js",
      "types": "./dist/constants.d.ts"
    },
    "./messages": {
      "import": "./dist/messages.js",
      "types": "./dist/messages.d.ts"
    },
    "./agent-inbox-interrupt": {
      "import": "./dist/agent-inbox-interrupt.js",
      "types": "./dist/agent-inbox-interrupt.d.ts"
    },
    "./caching": {
      "import": "./dist/caching.js",
      "types": "./dist/caching.d.ts"
    },
    "./configurable-metadata": {
      "import": "./dist/configurable-metadata.js",
      "types": "./dist/configurable-metadata.d.ts"
    },
    "./crypto": {
      "import": "./dist/crypto.js",
      "types": "./dist/crypto.d.ts"
    },
    "./git": {
      "import": "./dist/git.js",
      "types": "./dist/git.d.ts"
    },
    "./jwt": {
      "import": "./dist/jwt.js",
      "types": "./dist/jwt.d.ts"
    },
    "./github/auth": {
      "import": "./dist/github/auth.js",
      "types": "./dist/github/auth.d.ts"
    },
    "./github/allowed-users": {
      "import": "./dist/github/allowed-users.js",
      "types": "./dist/github/allowed-users.d.ts"
    },
    "./github/verify-user": {
      "import": "./dist/github/verify-user.js",
      "types": "./dist/github/verify-user.d.ts"
    },
    "./agent-mojo/types": {
      "import": "./dist/agent-mojo/types.js",
      "types": "./dist/agent-mojo/types.d.ts"
    },
    "./agent-mojo/local-mode": {
      "import": "./dist/agent-mojo/local-mode.js",
      "types": "./dist/agent-mojo/local-mode.d.ts"
    },
    "./agent-mojo/tasks": {
      "import": "./dist/agent-mojo/tasks.js",
      "types": "./dist/agent-mojo/tasks.d.ts"
    },
    "./agent-mojo/tools": {
      "import": "./dist/agent-mojo/tools.js",
      "types": "./dist/agent-mojo/tools.d.ts"
    },
    "./agent-mojo/models": {
      "import": "./dist/agent-mojo/models.js",
      "types": "./dist/agent-mojo/models.d.ts"
    },
    "./agent-mojo/mcp": {
      "import": "./dist/agent-mojo/mcp.js",
      "types": "./dist/agent-mojo/mcp.d.ts"
    },
    "./agent-mojo/llm-task": {
      "import": "./dist/agent-mojo/llm-task.js",
      "types": "./dist/agent-mojo/llm-task.d.ts"
    },
    "./agent-mojo/custom-node-events": {
      "import": "./dist/agent-mojo/custom-node-events.js",
      "types": "./dist/agent-mojo/custom-node-events.d.ts"
    },
    "./agent-mojo/manager/types": {
      "import": "./dist/agent-mojo/manager/types.js",
      "types": "./dist/agent-mojo/manager/types.d.ts"
    },
    "./agent-mojo/planner/types": {
      "import": "./dist/agent-mojo/planner/types.js",
      "types": "./dist/agent-mojo/planner/types.d.ts"
    },
    "./agent-mojo/reviewer/types": {
      "import": "./dist/agent-mojo/reviewer/types.js",
      "types": "./dist/agent-mojo/reviewer/types.d.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "files": [
    "dist/**/*"
  ]
}
