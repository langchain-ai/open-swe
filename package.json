{
  "name": "open-swe",
  "author": "LangChain",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "guard:scan": "node ./scripts/run-guard.mjs",
    "guard:no-daytona": "node ./scripts/run-guard.mjs",
    "prebuild": "yarn guard:scan",
    "dev": "turbo dev",
    "predev": "yarn guard:scan",
    "build": "turbo build",
    "preformat": "yarn guard:scan",
    "turbo:command": "turbo",
    "preturbo:command": "yarn guard:scan",
    "format": "turbo format",
    "preformat:check": "yarn guard:scan",
    "clean": "rm -rf .turbo || true && turbo clean",
    "preclean": "yarn guard:scan",
    "format:check": "turbo format:check",
    "lint": "turbo lint",
    "prelint": "yarn guard:scan",
    "lint:fix": "turbo lint:fix",
    "prelint:fix": "yarn guard:scan",
    "test": "turbo test",
    "pretest": "yarn guard:scan",
    "format:features": "node ./scripts/format-feature-graph.mjs",
    "sandbox:build": "docker build -f Dockerfile.sandbox -t open-swe-sandbox:local .",
    "stack:up": "docker compose -f compose.local.yaml up -d",
    "stack:down": "docker compose -f compose.local.yaml down"
  },
  "devDependencies": {
    "turbo": "^2.5.0",
    "typescript": "^5",
    "yaml": "^2.6.0"
  },
  "resolutions": {
    "@langchain/langgraph-sdk": "^0.0.95",
    "@langchain/core": "^0.3.65",
    "langsmith": "^0.3.48",
    "jsonwebtoken": "^9.0.2"
  },
  "packageManager": "yarn@3.5.1"
}
